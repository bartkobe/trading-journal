{
  "name": "trading-journal-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:legacy": "next dev",
    "dev:local": "bash ./scripts/dev-local.sh",
    "postinstall": "prisma generate",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "type-check:ci": "tsc --noEmit --strict --exactOptionalPropertyTypes --noImplicitReturns --noImplicitOverride --noPropertyAccessFromIndexSignature --noUncheckedIndexedAccess",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.918.0",
    "@aws-sdk/s3-request-presigner": "^3.918.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.18.0",
    "@tiptap/extension-link": "^3.9.0",
    "@tiptap/extension-placeholder": "^3.9.0",
    "@tiptap/react": "^3.9.0",
    "@tiptap/starter-kit": "^3.9.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.8.0",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.2",
    "next": "16.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.65.0",
    "recharts": "^3.3.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.9.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "dotenv-cli": "^7.4.1",
    "eslint": "^9.0.0",
    "eslint-config-next": "16.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "tailwindcss": "^4.1.0",
    "typescript": "^5.9.0",
    "undici": "^7.16.0",
    "whatwg-fetch": "^3.6.20"
  }
}
